// 1. Поменять местами содержимое переменных A и B и вывести новые значения A и B.

#include <iostream>
#include <stdio.h>
#include <locale.h>
using namespace std;
main()
{
	int a,b,c;
	cout <<"vvedite a & b chtobi pomenyat ih mestami"<<"\na=";
	cin>>a;
	cout<<"b=";
	cin>>b;
	c=a;
	a=b;
	b=c;
	cout<<"a="<<a<<"\nb="<<b;
	return 0;
}

